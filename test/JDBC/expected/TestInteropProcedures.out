CREATE PROCEDURE psql_interop_proc3()
AS
$$
DECLARE myvar varchar(10) = 'def';
BEGIN
UPDATE procTab1 SET c2=myvar WHERE c1 BETWEEN 2 AND 10;
INSERT INTO procTab1(c1,c2) values(12,'xyz');
END
$$ LANGUAGE PLPGSQL;
GO

