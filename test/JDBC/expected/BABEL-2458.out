dbo#!#dbo
~~END~~


SELECT COUNT(*) FROM sys.schemas where schema_name(schema_id(name)) = 'wqreqwreqwerqwereqwrqreqwrqwrqwerqewreqwreqwrewqreerweqrqwreqwreqreqewrqwereqrrereqwerqwereqwrw';
GO
