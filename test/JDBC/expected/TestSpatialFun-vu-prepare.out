CREATE VIEW point_equality1 AS SELECT STEquals( p1.location, p2.location ) AS equality FROM SPATIALPOINTGEOM_dt p1 CROSS JOIN SPATIALPOINTGEOM_dt p2;

CREATE VIEW isIn AS SELECT STContains( p1.location, p2.location ) AS isIN FROM SPATIALPOINTGEOM_dt p1 CROSS JOIN SPATIALPOINTGEOM_dt p2 ORDER BY p1.location.STX;

CREATE VIEW ValFromGeom AS SELECT location.STArea() FROM SPATIALPOINTGEOM_dt ORDER BY location.STX;

CREATE VIEW TextFromGeog AS SELECT  STArea(location) AS Area FROM SPATIALPOINTGEOG_dt ORDER BY location.Lat;

CREATE VIEW point_Equality AS SELECT STEquals( p1.location, p2.location ) AS Equality FROM SPATIALPOINTGEOG_dt p1 CROSS JOIN SPATIALPOINTGEOG_dt p2 ORDER BY p1.location.Lat;

CREATE VIEW point_in AS SELECT STContains( p1.location, p2.location ) AS isIn FROM SPATIALPOINTGEOG_dt p1 CROSS JOIN SPATIALPOINTGEOG_dt p2;
