-- tsql
-- Note : table types's database visibility has been already tested in sys-types.sql
select name
      , system_type_id
      , principal_id
      , max_length
      , precision
      , scale
      , collation_name
      , is_nullable
      , is_user_defined
      , is_assembly_type
      , default_object_id
      , rule_object_id
