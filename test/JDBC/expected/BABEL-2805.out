-- Regression tests for CROSS JOIN
create table t1 (a int, b int)
create table t2 (c int, d int)
insert into t1 values (1, 1),(2, 2)
insert into t2 values (3, 3),(4, 4)
go
~~ROW COUNT: 2~~

~~ROW COUNT: 2~~


select * from t1 cross join (values (3, 3),(4,4)) t2(c1, c2)
