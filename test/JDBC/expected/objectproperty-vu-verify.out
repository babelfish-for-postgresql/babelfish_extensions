-- =============== OwnerId ===============
-- Check for correct case
SELECT CASE
    WHEN OBJECTPROPERTY(OBJECT_ID('ownerid_schema.ownerid_table'), 'OwnerId')  = (SELECT principal_id 
            FROM sys.database_principals
            WHERE name = CURRENT_USER)
        Then 'SUCCESS'
    ELSE
        'FAILED'
END
GO
~~START~~
text
SUCCESS
~~END~~


