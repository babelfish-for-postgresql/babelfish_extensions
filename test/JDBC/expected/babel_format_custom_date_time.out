
-- Custom date formats
DECLARE @curr_date date;
SET @curr_date = '12/01/1999';
SELECT FORMAT(@curr_date, 'dd/yy/MM', 'dz-BT');
GO
~~ERROR (Code: 33557097)~~

~~ERROR (Message: The culture parameter "dz-BT" provided in the function call is not supported.)~~


