EXEC SP_EXECUTE @handle, @inner_handle OUT
BEGIN TRANSACTION;
EXEC SP_EXECUTE @inner_handle
ROLLBACK;
EXEC SP_UNPREPARE @inner_handle  -- unprepare inner first
EXEC SP_UNPREPARE @handle
