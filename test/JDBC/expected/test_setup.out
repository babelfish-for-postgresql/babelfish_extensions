-- tsql
CREATE TABLE onek (
	unique1		int,
	unique2		int,
	two			int,
	four		int,
	ten			int,
	twenty		int,
	hundred		int,
	thousand	int,
	twothousand	int,
	fivethous	int,
	tenthous	int,
	odd			int,
	even		int,
	stringu1	name,
	stringu2	name,
	string4		name
);
GO

-- bash   
/bin/bash input/pg_regress/load_onek.sh

-----copy onek table from /data/onek.data-----
\getenv pg_src PG_SRC
\set filename :pg_src '/src/test/regress/data/onek.data'
COPY master_dbo.onek FROM :'filename';
COPY 1000
----- done -----

-- psql
VACUUM ANALYZE master_dbo.onek;
GO

