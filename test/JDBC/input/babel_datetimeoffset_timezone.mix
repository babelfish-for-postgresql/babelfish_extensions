-- tsql
SELECT CAST('2000-06-01 00:00:00' AS DATETIMEOFFSET)
GO

-- psql
ALTER DATABASE jdbc_testdb SET timezone TO 'Australia/Sydney'
GO

-- terminate-tsql-conn
-- tsql
-- Reopen connection to refresh session time zone
SELECT current_setting('TIMEZONE')
GO
SELECT CAST('2000-06-01 00:00:00' AS DATETIMEOFFSET)
GO

-- psql
ALTER DATABASE jdbc_testdb RESET timezone
GO
