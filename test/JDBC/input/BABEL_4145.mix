-- tsql
EXEC sp_babelfish_configure 'repeatable_read_isolation', 'default', 'server'
EXEC sp_babelfish_configure 'serializable_isolation', 'default', 'server'
GO
-- terminate-tsql-conn

-- tsql
EXEC sp_babelfish_configure 'repeatable_read_isolation', 'pg_isolation'
EXEC sp_babelfish_configure 'serializable_isolation', 'pg_isolation'
GO
SELECT current_setting('babelfishpg_tsql.repeatable_read_isolation');
SELECT current_setting('babelfishpg_tsql.serializable_isolation');
GO
-- terminate-tsql-conn

